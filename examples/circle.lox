var start = ticks();
var radius = 10;
for (var i = 1; i < radius; i=i+1) {
    var w = floor(radius * sin(180/radius * i * 3.1415/180));
    for (var s = 0; s <= radius - w; s=s+1) print " ";
    for (var j = 1; j <= 2 * w; j=j+1) print "*";        
    println "";
}
print "Elapsed Time: ";
println ticks() - start;